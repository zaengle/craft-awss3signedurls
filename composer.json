{
    "name": "zaengle/craft-aws-s3-signed-urls",
    "description": "Signed URLs for AWS S3 Craft Asset Volumes, including the ability to limit access.",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "aws s3 signed urls"
    ],
    "support": {
        "docs": "https://github.com/zaengle/craft-aws-s3-signed-urls/blob/master/README.md",
        "issues": "https://github.com/zaengle/craft-aws-s3-signed-urls/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Zaengle Corp",
            "homepage": "https://zaengle.com"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0|^8.1",
        "craftcms/cms": "^4.1.0",
        "craftcms/aws-s3": "^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.4"
    },
    "autoload": {
        "psr-4": {
          "zaengle\\awss3signedurls\\": "src/"
        }
    },
    "extra": {
        "name": "AWS S3 Signed URLs",
        "handle": "aws-s3-signed-urls",
        "developer": "Zaengle Corp",
        "developerUrl": "https://zaengle.com",
        "documentationUrl": "https://github.com/zaengle/craft-aws-s3-signed-urls/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/zaengle/craft-aws-s3-signed-urls/master/CHANGELOG.md",
        "components": {
            "awsS3SignedUrlsService": "Sign"
        },
        "class": "zaengle\\awss3signedurls\\AwsS3SignedUrls"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyse",
        "fix-cs": "vendor/bin/php-cs-fixer fix src --allow-risky=yes"
    }
}
